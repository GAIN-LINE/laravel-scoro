    "description": "Laravel SDK wrapper for Scoro API",
    "autoload": {
        "psr-4": {
            "Scoro\\": "src/Scoro/",
            "ScoroLaravel\\": "src/ScoroLaravel/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Scoro\\ScoroServiceProvider"
            ],
            "aliases": {
                "Scoro": "Scoro\\Facades\\Scoro"
            }
        }
    }
}
